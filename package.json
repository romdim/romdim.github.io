    "watch:tailwind": "./node_modules/postcss-cli/bin/postcss src/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production ./node_modules/postcss-cli/bin/postcss src/tailwind.css -o static/index.css"
  },
